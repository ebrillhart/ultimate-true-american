<%= link_to "Signup with Facebook", "/auth/facebook", class: "btn btn-primary" %>


<%= bootstrap_form_for :user, url: signup_path do |f| %>
  <%= f.email_field :email %>
  <%= f.text_field :username %>
  <%= f.password_field :password %>
  <%= f.password_field :password_confirmation %>

  <%= f.primary 'Signup' %>
<% end %>